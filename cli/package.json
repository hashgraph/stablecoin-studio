{
  "name": "hedera-stable-coin-cli",
  "version": "0.0.1-SNAPSHOT",
  "description": "CLI for Hedera Stable Coin",
  "main": "./build/src/index.js",
  "bin": {
    "accelerator": "build/src/index.js"
  },
  "exports": {
    ".": "./build/src/index.js"
  },
  "scripts": {
    "start": "clear && npx tsc && node ./build/src/index.js",
    "start:wizard": "clear && npx tsc && node ./build/src/index.js wizard -cp './hsca-config.yaml' -n 'testnet'",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^8.0.0",
    "@types/jest": "~27.5",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "hedera-stable-coin-sdk": "file:../sdk",
    "jest": "^28.1.3",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "tsutils": "~3.21"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^5.0.1",
    "cli-table3": "^0.6.2",
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "figlet-promised": "^1.0.0",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "4.7.4"
  }
}
