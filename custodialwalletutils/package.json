{
  "name": "@hashgraph/hedera-custodians-integration",
  "version": "1.16.0",
  "description": "A library to provide integration with custodial wallets",
  "main": "./build/cjs/src/index.js",
  "module": "./build/esm/src/index.js",
  "types": "./build/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/src/index.js",
      "require": "./build/cjs/src/index.js"
    }
  },
  "files": [
    "./build"
  ],
  "homepage": "https://github.com/hashgraph/stablecoin-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashgraph/stablecoin-studio"
  },
  "bugs": {
    "url": "https://github.com/hashgraph/stablecoin-studio/issues"
  },
  "license": "ISC",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x86_64",
    "x64",
    "arm",
    "arm64"
  ],
  "scripts": {
    "start": "node build/src/index.js",
    "clean": "rimraf build coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "build": "rimraf build && npx tsc -p tsconfig.json && npx tsc -p tsconfig-cjs.json",
    "build:types": "npx tsc -p tsconfig.json --emitDeclarationOnly",
    "build:tsc": "rm -rf build && npx tsc -p tsconfig.json",
    "build:watch": "concurrently --kill-others --names \"ESM,CJS\" \"npx tsc -w -p tsconfig.json\" \"npx tsc -w -p tsconfig-cjs.json\"",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "prettier": "prettier --config .prettierrc --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-license-header": "^0.6.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "ts-jest": "^29.0.5",
    "babel-jest": "^29.3.1 ",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "@dfns/sdk": "^0.1.0-beta.5",
    "@dfns/sdk-keysigner": "^0.1.0-beta.5",
    "fireblocks-sdk": "^5.9.0",
    "@fireblocks/fireblocks-web3-provider": "^1.2.5"
  }
}
