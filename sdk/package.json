{
  "name": "hedera-stable-coin-sdk",
  "version": "0.0.1",
  "description": "",
  "main": "./build/cjs/src/index.js",
  "module": "./build/esm/src/index.js",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "files": [
    "./build"
  ],
  "readme": "README.md",
  "types": "./build/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/src/index.js",
      "require": "./build/cjs/src/index.js"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "~29.1",
    "@types/node": "~18",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.38",
    "@typescript-eslint/parser": "~5.38",
    "axios": "^0.27.2",
    "babel-jest": "29.1.2",
    "concurrently": "^7.4.0",
    "eslint": "~8.24",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.0",
    "format-imports": "^3.1.0",
    "hedera-stable-coin-contracts": "file:../contracts/build",
    "jest": "29.1",
    "prettier": "~2.7",
    "rimraf": "~3.0",
    "ts-jest": "29.0",
    "tsutils": "~3.21",
    "typed-emitter": "^2.1.0",
    "typescript": "4.8"
  },
  "scripts": {
    "start": "node build/src/index.js",
    "clean": "rimraf coverage build",
    "prebuild": "npm run lint",
    "postbuild": "node ./scripts/postbuild.js",
    "build": "rimraf build && npx tsc -p tsconfig.json && npx tsc -p tsconfig-cjs.json",
    "build:types": "npx tsc -p tsconfig.json --emitDeclarationOnly",
    "build:tsc": "rm -rf build && npx tsc -p tsconfig.json",
    "build:watch": "concurrently --kill-others --names \"ESM,CJS\" \"npx tsc -w -p tsconfig.json\" \"npx tsc -w -p tsconfig-cjs.json\"",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "npm run test:jest",
    "prettier": "prettier --config .prettierrc --write .",
    "format:imports": "npx format-imports ./src",
    "test:watch": "concurrently --kill-others \"npm run build:watch\" \"npm run test:jest:watch\"",
    "test:jest:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "test:jest": "NODE_OPTIONS=--experimental-vm-modules npx jest --ci --json --coverage --forceExit --testLocationInResults --outputFile=report.json"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "hashconnect": "file:../hashconnect/lib",
    "@hashgraph/hethers": "^1.2.2",
    "@hashgraph/sdk": "^2.18.2",
    "ts-xor": "^1.0.8",
    "tslib": "~2.4",
    "uuid": "^9.0.0",
    "web3": "^1.8.0"
  },
  "volta": {
    "node": "16.13.0"
  }
}
