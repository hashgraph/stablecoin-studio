{
	"name": "@hashgraph/stablecoin-dapp",
	"version": "1.25.0",
	"files": [
		"build/"
	],
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test --maxWorkers=25% --watchAll=false --cache=false",
		"test:watch": "react-app-rewired test --maxWorkers=50% --watchAll=true",
		"test:update": "react-app-rewired test -u --maxWorkers=50% --watchAll=false",
		"test:ci": "react-app-rewired test --ci --runInBand --watchAll=false --cache=false",
		"cleanCache": "react-app-rewired test --clearCache",
		"eject": "react-app-rewired eject",
		"lint": "eslint . --ext .ts --ext .mts --fix",
		"prettier": "prettier --config .prettierrc --write . --fix",
		"prettierCheck": "prettier --config .prettierrc --check",
		"pre-commit": "npm run lint && npm run prettier",
		"prepack": "npm run build"
	},
	"devDependencies": {
		"@babel/plugin-transform-private-property-in-object": "^7.25.7",
		"@types/node": "^18.11.18",
		"@types/react-toastify": "^4.1.0",
		"@types/redux-mock-store": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2 ",
		"assert": "^2.0.0",
		"buffer": "^6.0.3",
		"crypto-browserify": "^3.12.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.32.1",
		"https-browserify": "^1.0.0",
		"i18next": "^22.4.9",
		"i18next-browser-languagedetector": "^7.0.1",
		"os-browserify": "^0.3.0",
		"phosphor-react": "^1.4.1",
		"prettier": "^2.8.3",
		"process": "^0.11.10",
		"react-app-rewired": "^2.2.1",
		"react-i18next": "^12.1.4",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"ts-jest": "^29.0.5",
		"url": "^0.11.0"
	},
	"dependencies": {
		"@chakra-ui/icons": "~2.0.17",
		"@chakra-ui/react": "~2.6.1",
		"@emotion/react": "^11.0.0",
		"@emotion/styled": "^11.0.0 ",
		"@hashgraph/stablecoin-npm-sdk": "file:./../sdk",
		"@reduxjs/toolkit": "^1.9.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "14.0.0-alpha.3",
		"@testing-library/user-event": "^14.4.2",
		"@types/jest": "^29.2.6",
		"@types/react": "^18.0.27",
		"@types/react-datepicker": "^4.8.0",
		"@types/react-dom": "^18.0.6",
		"@types/react-redux": "^7.1.25",
		"axios": "^1.2.3",
		"browserify-zlib": "^0.2.0",
		"chakra-react-select": "~4.5.0",
		"framer-motion": "^8.5.2",
		"maskdata": "^1.1.10",
		"moment": "^2.29.4",
		"named-urls": "^2.0.1",
		"path-browserify": "^1.0.1",
		"react": "^18.2.0",
		"react-datepicker": "^4.8.0",
		"react-device-detect": "^2.2.3",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.42.1",
		"react-number-format": "^5.1.3",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.7.0",
		"react-scripts": "5.0.1",
		"react-switch": "^7.0.0",
		"react-toastify": "^9.1.1",
		"redux-mock-store": "^1.5.4",
		"typescript": "^4.9.4",
		"web-vitals": "^3.1.1",
		"webpack": "^5.75.0"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"typesVersions": {
		"*": {
			"*": [
				"./lib/*"
			]
		}
	},
	"types": "./lib/*.d.ts",
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "jsdom",
		"clearMocks": true,
		"testTimeout": 60000,
		"modulePaths": [
			"<rootDir>/src"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/src/test/setupTests.tsx"
		],
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.(m)?js$": "$1",
			"axios": "<rootDir>/node_modules/axios/dist/node/axios.cjs",
			"blade": "<rootDir>/src/mocks/blade-sdk-mock.js",
			"hedera-wallet-connect": "<rootDir>/src/mocks/hedera-wallet-connect-sdk-mock.js",
			"fireblocks-sdk": "<rootDir>/src/mocks/fireblocks-sdk-mock.js"
		},
		"transform": {
			"^.+\\.ts?$": "ts-jest",
			"^.+\\.[t|j]sx?$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
		]
	}
}
